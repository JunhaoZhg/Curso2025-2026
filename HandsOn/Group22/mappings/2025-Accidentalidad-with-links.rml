### TriplesMap Distrito

<#DistritoMapping>
  rml:logicalSource [
    rml:source "2025-Accidentalidad-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://smartaccidents.es/distrito/{cod_distrito}" ;
    rr:class trafico:Distrito
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rml:reference "distrito" ; rr:datatype xsd:string ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate trafico:codigoDistrito ;
    rr:objectMap [ rml:reference "cod_distrito" ; rr:datatype xsd:string ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "same_as_wikidata_district" ; rr:termType rr:IRI ]
  ] .