##################################################################
# TriplesMap: Episodios de Ozono (con enlaces reconciliados)
##################################################################
<#OzonoTriplesMap-with-links>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "csv/JUEGO-DATOS-activacion-protocolo-O3-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "http://example.org/resource/EpisodioOzono/{FECHA INICIO}" ;
    rr:class vocab:EpisodioOzono
  ] ;

  rr:predicateObjectMap [
    rr:predicate vocab:inicio ;
    rr:objectMap [ rml:reference "FECHA INICIO" ; rr:datatype xsd:dateTime ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate vocab:fin ;
    rr:objectMap [ rml:reference "FECHA FIN" ; rr:datatype xsd:dateTime ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate vocab:escenario ;
    rr:objectMap [ rml:reference "EPISODIO OZONO" ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate vocab:medidaPoblacion ;
    rr:objectMap [ rml:reference "MEDIDA POBLACION" ]
  ] ;

  # ðŸ†• Enlace externo (columna reconciliada)
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "EPISODIO URI" ; rr:termType rr:IRI ]
  ] .