@prefix rr:    <http://www.w3.org/ns/r2rml#> .
@prefix rml:   <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:    <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix vocab: <http://example.org/vocab#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@base <http://example.org/resource/> .

##################################################################
# TriplesMap: Calidad del Aire (con enlaces reconciliados)
##################################################################
<#CalidadAireTriplesMap-with-links>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "csv/CALIDAD-AIRE-HORARIOS20250813-with-links.csv" ;
    rml:referenceFormulation ql:CSV ;
    rml:delimiter ";"
  ] ;

  rr:subjectMap [
    rr:template "http://example.org/resource/MedicionAire/{ESTACION}/{MAGNITUD}/{Fecha}" ;
    rr:class vocab:MedicionAire
  ] ;

  rr:predicateObjectMap [ rr:predicate vocab:provincia ; rr:objectMap [ rml:reference "PROVINCIA" ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:municipio ; rr:objectMap [ rml:reference "MUNICIPIO" ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:estacion ; rr:objectMap [ rml:reference "ESTACION" ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:magnitud ; rr:objectMap [ rml:reference "MAGNITUD" ] ] ;
  rr:predicateObjectMap [ rr:predicate owl:sameAs ; rr:objectMap [ rml:reference "MAGNITUD URI" ; rr:termType rr:IRI ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:puntoMuestreo ; rr:objectMap [ rml:reference "PUNTO_MUESTREO" ] ] ;
  rr:predicateObjectMap [ rr:predicate vocab:fecha ; rr:objectMap [ rml:reference "Fecha" ; rr:datatype xsd:dateTime ] ] ;
