PREFIX schema: <https://schema.org/>
PREFIX ns:     <https://www.roadsafetydata.com/ontology#>
PREFIX owl:    <http://www.w3.org/2002/07/owl#>
PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?event ?vehRes ?vehLink
WHERE {
  ?event a schema:Event ;
         ns:involvesPerson ?p .
  ?p ns:usesVehicleType ?vehRes .
  OPTIONAL { ?vehRes owl:sameAs ?vehLink . }
}
LIMIT 100

