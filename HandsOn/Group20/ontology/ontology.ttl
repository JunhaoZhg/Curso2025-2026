@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix mod: <https://w3id.org/mod#> .
@prefix my: <https://data.smartcitymadrid.es/ontology/parking#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:identifier a owl:DatatypeProperty ;
    rdfs:label "identifier" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:int .

dcterms:license a owl:AnnotationProperty .

dcterms:modified a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

schema1:address a owl:ObjectProperty ;
    rdfs:label "address" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range schema1:PostalAddress .

schema1:addressCountry a owl:ObjectProperty ;
    rdfs:label "address country" ;
    rdfs:domain schema1:PostalAddress ;
    rdfs:range schema1:Country .

schema1:addressLocality a owl:ObjectProperty ;
    rdfs:label "address locality" ;
    rdfs:domain schema1:PostalAddress ;
    rdfs:range schema1:City .

schema1:addressRegion a owl:ObjectProperty ;
    rdfs:label "address region" ;
    rdfs:domain schema1:PostalAddress ;
    rdfs:range my:AutonomousCommunity .

schema1:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:string .

schema1:postalCode a owl:DatatypeProperty ;
    rdfs:label "postal code" ;
    rdfs:domain schema1:PostalAddress ;
    rdfs:range xsd:int .

schema1:streetAddress a owl:DatatypeProperty ;
    rdfs:label "street address" ;
    rdfs:domain schema1:PostalAddress ;
    rdfs:range xsd:string .

geo:asWKT a owl:DatatypeProperty ;
    rdfs:label "as w k t" ;
    rdfs:domain geo:Point ;
    rdfs:range geo:wktLiteral .

geo:hasGeometry a owl:ObjectProperty ;
    rdfs:label "has geometry" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range geo:Point .

geo:lat a owl:DatatypeProperty ;
    rdfs:label "lat" ;
    rdfs:domain geo:Point ;
    rdfs:range xsd:double .

geo:long a owl:DatatypeProperty ;
    rdfs:label "long" ;
    rdfs:domain geo:Point ;
    rdfs:range xsd:double .

owl:versionIRI a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

vcard:email a owl:DatatypeProperty ;
    rdfs:label "email" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:string .

my:containedInCity a owl:ObjectProperty ;
    rdfs:label "contained in city" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range schema1:City ;
    rdfs:subPropertyOf schema1:containedInPlace .

my:containedInCountry a owl:ObjectProperty ;
    rdfs:label "contained in country" ;
    rdfs:domain my:AutonomousCommunity ;
    rdfs:range schema1:Country ;
    rdfs:subPropertyOf schema1:containedInPlace .

my:containedInNation a owl:ObjectProperty ;
    rdfs:label "contained in nation" ;
    rdfs:domain schema1:City ;
    rdfs:range schema1:Country ;
    rdfs:subPropertyOf schema1:containedInPlace .

my:containedInRegion a owl:ObjectProperty ;
    rdfs:label "contained in region" ;
    rdfs:domain schema1:City ;
    rdfs:range my:AutonomousCommunity ;
    rdfs:subPropertyOf schema1:containedInPlace .

my:hasCategory a owl:DatatypeProperty ;
    rdfs:label "has category" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:string .

my:hasCategoryCode a owl:DatatypeProperty ;
    rdfs:label "has category code" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:int .

my:hasFamily a owl:DatatypeProperty ;
    rdfs:label "has family" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:int .

my:hasType a owl:DatatypeProperty ;
    rdfs:label "has type" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:string .

my:hasURLIcon a owl:DatatypeProperty ;
    rdfs:label "has u r l icon" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:string .

my:isEMTParking a owl:DatatypeProperty ;
    rdfs:label "is e m t parking" ;
    rdfs:domain schema1:ParkingFacility ;
    rdfs:range xsd:boolean .

schema1:AdministrativeArea a owl:Class ;
    rdfs:label "Administrative Area" .

schema1:CivicStructure a owl:Class ;
    rdfs:label "Civic Structure" ;
    rdfs:subClassOf schema1:Place .

my: a owl:Ontology ;
    dcterms:contributor "Laura Gilgado Romero"@es,
        "María Jiménez Álvarez"@es,
        "Nerea Tanarro Ferrero"@es,
        "Sergio Caro Rodríguez"@es,
        "Sergio Zaballos Herrera"@es ;
    dcterms:created "2025-10-07"^^xsd:date ;
    dcterms:creator "Group 20 in Web Semantics, Linked Data and Knowledge Graphs, UPM."@en ;
    dcterms:description "Ontology that describes parking facilities managed by EMT Madrid, including their locations and attributes."@en ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:modified "2025-11-07"^^xsd:date ;
    dcterms:title "Parking ontology"@en ;
    vann:preferredNamespacePrefix "my" ;
    vann:preferredNamespaceUri my: ;
    owl:versionIRI <https://data.smartcitymadrid.es/ontology/parking/1.0.0> ;
    owl:versionInfo "1.0.0" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

schema1:Place a owl:Class ;
    rdfs:label "Place" .

schema1:Country a owl:Class ;
    rdfs:label "Country" .

schema1:containedInPlace a owl:ObjectProperty ;
    rdfs:label "contained in place" ;
    rdfs:domain schema1:Place ;
    rdfs:range schema1:Place .

geo:Point a owl:Class ;
    rdfs:label "Point" .

my:AutonomousCommunity a owl:Class ;
    rdfs:label "Autonomous Community" ;
    rdfs:subClassOf schema1:AdministrativeArea .

schema1:City a owl:Class ;
    rdfs:label "City" .

schema1:PostalAddress a owl:Class ;
    rdfs:label "Postal Address" .

schema1:ParkingFacility a owl:Class ;
    rdfs:label "Parking Facility" ;
    rdfs:subClassOf schema1:CivicStructure .

[] a owl:AllDisjointClasses ;
    owl:members ( schema1:City my:AutonomousCommunity schema1:Country ) .
